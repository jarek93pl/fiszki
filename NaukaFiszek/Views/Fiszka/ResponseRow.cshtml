@{
    Layout = null;
}
@model DTO.Models.FicheResponse
<tr id=@(Model.Id == 0 ? "IsNewFicheResponseRow":Model.Id.ToString()) class="responseTableRow">
    <td class="NameFicheResponseRow" id="xlis">
        @Model.Name
    </td>
    <td class="TypeFichRow">@Model.ContentTypeToDispley</td>
    <td class="IsCorectFichRow">@(Model.IsCorect ? "Tak" : "Nie")</td>
    <td>
        <a class="EditResponse" href="edit">Edytuj</a>
        <a class="DeleteResponse" href="delete">Usuń</a>
    </td>
    @Html.HiddenFor(model => model.IdFile, new { @class = "IdFileResponseRow" })
</tr>
