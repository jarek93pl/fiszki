@{
    Layout = null;
}
@model NaukaFiszek.Models.SetFicheDetails
<label></label>
<p>
    @Html.ActionLink("Dodaj Fiszke", "Dodaj","Fiszka")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fiches.First().Prompt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fiches.First().Response)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fiches.First().NameType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fiches.First().NameTypePrompt)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Fiches)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Prompt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Response)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameTypePrompt)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
<script src="~/Scripts/Zestaw/Edytuj.js"></script>